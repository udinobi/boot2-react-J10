{
  "author": "udinobi",
  "description": "PoC project with Spring Boot 2 (Java 10) as backend and React/Redux (Typescript 2.9) as SPA frontend.",
  "license": "MIT",
  "main": "index.tsx",
  "name": "frontend",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "chrome": "yarn css && BROWSER=google-chrome-stable env-cmd .env.dev react-app-rewired start --scripts-version react-scripts-ts",
    "chrome:staging": "yarn css && BROWSER=google-chrome-stable env-cmd .env.staging react-app-rewired start --scripts-version react-scripts-ts",
    "chrome:prod": "yarn css && BROWSER=google-chrome-stable env-cmd .env.prod react-app-rewired start --scripts-version react-scripts-ts",
    "css": "./node_modules/node-sass-chokidar/bin/node-sass-chokidar -o src/style --include-path ./node_modules src/style",
    "start": "yarn css && env-cmd .env.dev react-app-rewired start --scripts-version react-scripts-ts",
    "start:staging": "yarn css && env-cmd .env.staging react-app-rewired start --scripts-version react-scripts-ts",
    "start:prod": "yarn css && env-cmd .env.prod react-app-rewired start --scripts-version react-scripts-ts",
    "build": "yarn css && react-app-rewired build --scripts-version react-scripts-ts",
    "analyze": "source-map-explorer build/static/js/main.*",
    "test": "env-cmd .env.testing react-app-rewired test --env=jsdom --scripts-version react-scripts-ts",
    "coverage": "env-cmd .env.testing react-app-rewired test --env=jsdom --scripts-version react-scripts-ts -- --coverage",
    "eject": "react-scripts-ts eject"
  },
  "bugs": {
    "url": "https://github.com/udinobi/boot2-react-J10/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/udinobi/boot2-react-J10.git"
  },
  "dependencies": {
    "antd": "^3.6.4",
    "axios": "^0.18.0",
    "hashed": "^4.3.1",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.21",
    "ol": "4.6.5",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-infinite-scroller": "^1.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "2.16.0",
    "react-tooltip": "^3.6.1",
    "redux": "^4.0.0",
    "redux-observable": "1.0.0",
    "redux-thunk": "^2.3.0",
    "rxjs": "^6.2.1",
    "styled-components": "^3.3.3",
    "ts-option": "^1.1.5"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.11",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^23.1.3",
    "@types/moment-timezone": "^0.5.6",
    "@types/node": "^10.5.1",
    "@types/ol": "^4.6.2",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "@types/react-infinite-scroller": "^1.0.6",
    "@types/react-redux": "^6.0.3",
    "@types/react-router-dom": "^4.2.7",
    "@types/react-test-renderer": "^16.0.1",
    "@types/react-tooltip": "^3.3.5",
    "@types/redux-mock-store": "^1.0.0",
    "check-dependencies": "^1.1.0",
    "env-cmd": "^8.0.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest-environment-jsdom": "^23.2.0",
    "jest-enzyme": "^6.0.2",
    "jest-styled-components": "^5.0.1",
    "jsdom": "^11.11.0",
    "node-sass-chokidar": "^1.3.0",
    "react-app-rewired": "^1.5.2",
    "react-test-renderer": "^16.4.1",
    "redux-devtools-extension": "^2.13.5",
    "redux-mock-store": "^1.5.3",
    "source-map-explorer": "^1.5.0",
    "ts-import-plugin": "^1.5.4",
    "typescript": "^2.9.2"
  }
}
